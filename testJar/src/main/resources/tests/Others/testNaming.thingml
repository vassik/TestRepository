import "../thingml.thingml"

thing TestNaming includes Test 
@test "tt # (AE|EA)BCD"
{
    property A : Char = '\'A\''
    
	statechart TestNaming init A {
	                    
        property A : Char = '\'A\''
                            
		state A {
            property A : Char = '\'A\''
    
			on entry do
                var A : Char = '\'A\''
                harnessOut!testOut('\'A\'')
            end
			transition t -> C
			event m : harnessIn?testIn
			guard m.c == '\'t\''
		}
		
        composite state C init A {
            on entry harnessOut!testOut('\'B\'')
            
            state A {
			on entry do
                var A : Char = '\'A\''
                harnessOut!testOut('\'C\'')
            end
			transition t -> C
			event m : harnessIn?testIn
			guard m.c == '\'t\''
		    } 

   		   state C {
			on entry harnessOut!testOut('\'D\'')
		  }   
        }

		
		region A init A {	
			state A {
				on entry do
                    var A : Char = '\'A\''
                    harnessOut!testOut('\'E\'')
                end
			}
		}			
	}
}
